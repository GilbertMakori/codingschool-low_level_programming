#include "holberton.h"

/**
 *
 * add() to add two integers and return the result
 * num1 for operand number one
 * num2 for operand number two
 * result integer variable is sum of num1 and num2
 * return for result
 *
 */
int add(num1, num2)
{
	int result;

	result = num1 + num2;
	return (result);
}
