#include <stdio.h>
#include <stdlib.h>

/**
 * main - function with two argument
 * @argc: int type argument
 * @argv: char pointer type argument
 *
 * Description: adds positive numbers
 * Return: sum value
 */
int main(int argc, char* argv[])
{
	int i, j;

	if(argc > 2)
	{
		for(i = 1; i < argc; i++)
			for(j = 0; argv[i][j] != '\0'; j++)
			{
				printf("%i", atoi(argv[i][j]));
			}
	}
}
